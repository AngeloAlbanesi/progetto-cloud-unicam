@using ProvinciaTrentoHotel.ViewModels;
@using FunzioniDatiHotell.Modelli;
@model HomeElencoHotelViewModel



@{
    Layout = null;
}

<html>
<head>
    <title> Elenco Hotel</title>
</head>
<body>
    <h1>Elenco Strutture Alberghiere Provincia Autonoma Di Trento</h1>
</body>
<table>
    <thead>
        <tr>
            <th>COMUNE</th>
            <th>NOME STRUTTURA</th>
            <th>TIPOLOGIA</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var Hotel in Model.ElencoHotel)
        {
            <tr>
                <td>@Hotel.Comune</td>
                <td>@Hotel.DenominazioneStruttura</td>
                <td>@Hotel.Tipologia</td>
            </tr>
        }
    </tbody>
</table>

</html>

